{
	"name": "nubium/api-frame",
	"description": "API REST basic application frame",
	"license": "proprietary",
	"type": "library",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Nubium\\ApiFrame\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Nubium\\ApiFrame\\Tests\\": "tests/"
		}
	},
	"require": {
		"php": "^7.4",
		"apitte/core": "^0.6",
		"apitte/negotiation": "^0.6.0",
		"league/openapi-psr7-validator": "^0.10.0",
		"nette/utils": "^3.0 || ~2.5",
		"psr/http-message": "^1.0",
		"psr/http-server-middleware": "^1.0",
		"psr/log": "^1.0",
		"spareparts/enum": "~1.1",
		"symfony/validator": "~4.4.4",
		"zircote/swagger-php": "^3.2.0"
	},
	"require-dev": {
		"phpstan/extension-installer": "^1.0",
		"phpstan/phpstan": "^0.12.85",
		"phpstan/phpstan-phpunit": "^0.12.18",
		"phpunit/phpunit": "^9.5"
	}
}
